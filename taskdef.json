{
    "family": "sotetsu-lab-v3-api",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::442730633672:role/sotetsu-lab-v3-ecs-task-execution",
    "cpu": "2048",
    "memory": "300",
    "requiresCompatibilities": ["EC2"],
    "containerDefinitions": [
        {
            "name": "sotetsu-lab-v3-api",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "protocol": "tcp",
                    "hostPort": 3000,
                    "containerPort": 3000
                }
            ],
            "environment": [
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "CORS_HEADER_ORIGIN",
                    "value": "https://v3.sotetsu-lab.com"
                }
            ],
            "secrets": [
                {
                    "name": "DATABASE_URL",
                    "valueFrom": "sotetsu-lab-v3-api-database-url"
                },
                {
                    "name": "COGNITO_USERPOOL_ID",
                    "valueFrom": "sotetsu-lab-v3-auth-cognito-userpool-id"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/sotetsu-lab-v3-api",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
