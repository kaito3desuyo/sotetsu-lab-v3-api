{
    "name": "sotetsu-lab-v3-api",
    "version": "3.3.1",
    "description": "",
    "author": "kaito3desuyo",
    "license": "MIT",
    "scripts": {
        "preinstall": "typesync || :",
        "nest": "node --max_old_space_size=4096 ./node_modules/.bin/nest",
        "prebuild": "rimraf dist",
        "build": "npm run nest -- build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "npm run nest -- start",
        "start:dev": "npm run nest -- start --watch",
        "start:debug": "npm run nest -- start --debug --watch",
        "start:prod": "node dist/main.js",
        "deploy": "npm run build && serverless deploy",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "copy-secrets": "mkdir dist && cp -rv src/secrets dist/secrets/",
        "typeorm": "typeorm-ts-node-commonjs",
        "migration:create": "npm run typeorm -- migration:create",
        "migration:generate": "npm run typeorm -- migration:generate -d src/core/utils/data-source.ts",
        "migration:run": "npm run typeorm -- migration:run -d src/core/utils/data-source.ts",
        "migration:revert": "npm run typeorm -- migration:revert -d src/core/utils/data-source.ts"
    },
    "dependencies": {
        "@dataui/crud": "^5.3.4",
        "@dataui/crud-request": "^5.3.4",
        "@dataui/crud-typeorm": "^5.3.4",
        "@h4ad/serverless-adapter": "^3.2.0",
        "@nestjs/common": "^8.4.7",
        "@nestjs/core": "^8.4.7",
        "@nestjs/platform-express": "^8.4.7",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/typeorm": "^8.1.4",
        "aws-jwt-verify": "^4.0.1",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.14.1",
        "compression": "^1.7.4",
        "dayjs": "^1.11.12",
        "helmet": "^5.1.1",
        "just-omit": "^2.2.0",
        "lodash": "^4.17.21",
        "nest-commander": "^2.5.0",
        "nest-router": "^1.0.9",
        "nestjs-pino": "^4.1.0",
        "pg": "^8.12.0",
        "pino-http": "^10.2.0",
        "pluralize": "^8.0.0",
        "reflect-metadata": "^0.1.12",
        "typeorm": "^0.3.20",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.2.8",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^8.4.7",
        "@serverless/typescript": "^3.38.0",
        "@types/compression": "^1.7.5",
        "@types/eslint": "^9.6.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@types/lodash": "^4.17.7",
        "@types/node": "^22.4.0",
        "@types/pg": "^8.11.6",
        "@types/pluralize": "^0.0.33",
        "@types/prettier": "^2.7.3",
        "@types/serverless": "^3.12.22",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.1.0",
        "@typescript-eslint/parser": "^8.1.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "jest": "^29.7.0",
        "pino-pretty": "^11.2.2",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "serverless": "^3.39.0",
        "serverless-deployment-bucket": "^1.6.0",
        "serverless-layers": "^2.7.0",
        "serverless-plugin-warmup": "^8.3.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.7.4",
        "typesync": "^0.13.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "volta": {
        "node": "20.16.0"
    }
}
