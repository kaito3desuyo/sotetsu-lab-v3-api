{
    "name": "sotetsu-lab-v3-api",
    "version": "3.0.0",
    "description": "",
    "author": "kaito3desuyo",
    "license": "MIT",
    "scripts": {
        "preinstall": "typesync || :",
        "nest": "node --max_old_space_size=4096 ./node_modules/.bin/nest",
        "prebuild": "rimraf dist",
        "build": "npm run nest -- build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "npm run nest -- start",
        "start:dev": "npm run nest -- start --watch",
        "start:debug": "npm run nest -- start --debug --watch",
        "start:prod": "node dist/main.js",
        "deploy": "npm run build && serverless deploy",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "copy-secrets": "mkdir dist && cp -rv src/secrets dist/secrets/",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm --config ./src/core/config/ormconfig.ts",
        "migration:create": "npm run typeorm -- migration:create",
        "migration:generate": "npm run typeorm -- migration:generate",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert"
    },
    "dependencies": {
        "@h4ad/serverless-adapter": "^3.1.0",
        "@nestjs/common": "^8.4.7",
        "@nestjs/core": "^8.4.7",
        "@nestjs/jwt": "^8.0.1",
        "@nestjs/passport": "^8.2.2",
        "@nestjs/platform-express": "^8.4.7",
        "@nestjs/swagger": "^5.2.1",
        "@nestjs/typeorm": "^8.0.4",
        "@nestjsx/crud": "^4.6.2",
        "@nestjsx/crud-request": "^4.6.2",
        "@nestjsx/crud-typeorm": "^4.6.2",
        "class-transformer": "^0.3.1",
        "class-validator": "^0.12.2",
        "compression": "^1.7.4",
        "dayjs": "^1.10.7",
        "helmet": "^4.2.0",
        "jsonwebtoken": "^8.5.1",
        "jwks-rsa": "^2.0.3",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.32",
        "nest-router": "^1.0.9",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "pg": "^8.5.1",
        "pluralize": "^8.0.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "typeorm": "^0.2.41",
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^8.2.8",
        "@nestjs/schematics": "^8.0.11",
        "@nestjs/testing": "^8.4.7",
        "@serverless/typescript": "^3.30.1",
        "@types/aws-lambda": "^8.10.125",
        "@types/compression": "^1.7.2",
        "@types/eslint": "^7.2.13",
        "@types/eslint-config-prettier": "^6.11.2",
        "@types/jsonwebtoken": "^8.5.3",
        "@types/lodash": "^4.14.170",
        "@types/passport": "^0.4.7",
        "@types/passport-jwt": "^3.0.5",
        "@types/pg": "^8.6.0",
        "@types/pluralize": "^0.0.29",
        "@types/prettier": "2.2.0",
        "@types/rimraf": "^3.0.0",
        "@types/serverless": "^3.12.17",
        "@types/supertest": "2.0.11",
        "@types/tsc-watch": "4.2.0",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^6.9.1",
        "@typescript-eslint/parser": "^6.9.1",
        "eslint": "^8.53.0",
        "eslint-config-prettier": "^8.10.0",
        "jest": "26.6.3",
        "prettier": "2.8.8",
        "serverless": "^3.35.2",
        "serverless-deployment-bucket": "^1.6.0",
        "serverless-layers": "^2.7.0",
        "supertest": "6.0.1",
        "ts-jest": "26.4.4",
        "ts-node": "9.0.0",
        "tsconfig-paths": "3.9.0",
        "typescript": "^4.3.5",
        "typesync": "^0.11.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "volta": {
        "node": "20.8.1"
    }
}
