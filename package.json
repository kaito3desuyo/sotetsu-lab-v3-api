{
    "name": "sotetsu-lab-v3-api",
    "version": "1.0.1",
    "description": "",
    "author": "kaito3desuyo",
    "license": "MIT",
    "scripts": {
        "build": "rimraf dist && npm run copy-secrets && tsc -p tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
        "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
        "start:prod": "node dist/main.js",
        "lint": "tslint -p tsconfig.json -c tslint.json",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "copy-secrets": "mkdir dist && cp -rv src/secrets dist/secrets/",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
        "migration:create": "npm run typeorm -- migration:create --config src/db/ormconfig.ts --connection  --name ",
        "migration:generate": "npm run typeorm -- migration:generate --config src/db/ormconfig.ts --connection  --name ",
        "migration:run": "npm run typeorm -- migration:run"
    },
    "dependencies": {
        "@nestjs/common": "^6.10.3",
        "@nestjs/core": "^6.10.3",
        "@nestjs/platform-express": "^6.10.3",
        "@nestjs/platform-socket.io": "^6.10.3",
        "@nestjs/typeorm": "^6.2.0",
        "@nestjs/websockets": "^6.10.3",
        "@nestjsx/crud": "^4.2.0",
        "@nestjsx/crud-typeorm": "^4.2.0",
        "@types/moment-timezone": "^0.5.12",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "jsonwebtoken": "^8.5.1",
        "jwk-to-pem": "^2.0.1",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.27",
        "nest-router": "^1.0.9",
        "nestjs-typeorm-paginate": "^1.0.2",
        "pg": "^7.12.1",
        "pluralize": "^8.0.0",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.2",
        "rxjs": "^6.3.3",
        "typeorm": "^0.2.21",
        "uuid": "^3.4.0"
    },
    "devDependencies": {
        "@nestjs/testing": "6.6.7",
        "@types/express": "4.16.1",
        "@types/jest": "24.0.11",
        "@types/jsonwebtoken": "^8.3.3",
        "@types/lodash": "^4.14.136",
        "@types/node": "11.13.4",
        "@types/socket.io": "^2.1.2",
        "@types/supertest": "2.0.7",
        "@types/uuid": "^3.4.7",
        "jest": "24.7.1",
        "prettier": "1.17.0",
        "supertest": "4.0.2",
        "ts-jest": "24.0.2",
        "ts-node": "8.1.0",
        "tsc-watch": "2.2.1",
        "tsconfig-paths": "3.8.0",
        "tslint": "5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "3.6.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
